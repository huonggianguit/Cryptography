    <None Remove="Assets\Logo.png" />
    <None Remove="Assets\Logo2.png" />
    <None Remove="Assets\PassIcon.png" />
    <None Remove="Assets\Thumb_Icon_Default.png" />
    <None Remove="Assets\Thumb_Icon_MouseOver.png" />
    <None Remove="Assets\ToggleBut1.png" />
    <None Remove="Assets\ToggleBut2.png" />
    <None Remove="Assets\Up_Arrow_MouseOver.png" />
    <None Remove="Assets\UserIcon.png" />
